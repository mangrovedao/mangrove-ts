{
  "entryPoints": ["src/index.ts"],
  "exclude": [
    "node_modules/ethers/**/*",
    "src/types/Mangrove.ts",
    "src/types/**/*",
    "src/types/typechain/Mangrove.ts"
  ],
  "intentionallyNotExported": [
